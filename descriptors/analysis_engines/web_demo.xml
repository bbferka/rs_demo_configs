<?xml version="1.0" encoding="UTF-8"?>
<taeDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.cpp</frameworkImplementation>
  <primitive>false</primitive>
  <analysisEngineMetaData>
    <name>web_demo</name>
    <description>pipeline for the ease kickoff pnp demo</description>
    <version>1.0</version>
    <vendor/>
      <configurationParameters searchStrategy="none">
      <configurationParameter>
        <name>camera_config_files</name>
        <description>path to config file that holds info about camera topics and tf frame names</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>CollectionReader/camera_config_files</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>clearStorageOnStart</name>
        <description/>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>StorageWriter/clearStorageOnStart</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>storagedb</name>
        <description/>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>StorageWriter/db</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>hsvFilter</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>ImageSegmentationAnnotator/hsvFilter</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>segmMode</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>ImageSegmentationAnnotator/segmMode</parameter>
        </overrides>
      </configurationParameter>
      
      <configurationParameter>
        <name>cluster_min_size</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>PointCloudClusterExtractor/cluster_min_size</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>minSize</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>ImageSegmentationAnnotator/minSize</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>DeCafH5File</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>DeCafClassifier/DeCafH5File</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>DeCafListFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>DeCafClassifier/DeCafListFile</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>DeCafKDTreeIndices</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>DeCafClassifier/DeCafKDTreeIndices</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>border</name>
        <description>Border for semantic map regions.</description>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>RegionFilter/border</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>enable_change_detection</name>
        <description>Enable filtering of frames based on changes.</description>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>RegionFilter/enable_change_detection</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>projectOnPlane</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>Cluster3DGeometryAnnotator/projectOnPlane</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>sor_filter</name>
        <description>Statistical Outlier removal filter. Recomended to use with Kinect 2.
        Removes spurious points, making bb estimation more stable</description>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>Cluster3DGeometryAnnotator/sor_filter</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>overwriteExistingPoseEstimate</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>Cluster3DGeometryAnnotator/overwriteExistingPoseEstimate</parameter>
        </overrides>
      </configurationParameter>

      <configurationParameter>
        <name>defaultRegions</name>
        <description>Region entry from the semantic map based on which to filter the cloud (e.g. CounterTop or Drawers etc.) </description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>RegionFilter/defaultRegions</parameter>
        </overrides>
      </configurationParameter>

    </configurationParameters>

    <configurationParameterSettings>

      <nameValuePair>
        <name>defaultRegions</name>
        <value>
        <array>
          <string>kitchen_sink_block_counter_top</string>
          <string>kitchen_island_counter_top</string>
        </array>
        </value>
      </nameValuePair>

    <nameValuePair>
      <name>overwriteExistingPoseEstimate</name>
      <value>
        <boolean>false</boolean>
      </value>
    </nameValuePair>

      <nameValuePair>
        <name>sor_filter</name>
        <value>
          <boolean>true</boolean>
        </value>
      </nameValuePair>

      <nameValuePair>
        <name>projectOnPlane</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>

   <nameValuePair>
      <name>DeCafH5File</name>
      <value>
        <string>/caffe/extracted_feats/pnp_exp_bvlc_fc7/CNN_pnp_breakfast_1.hdf5</string>
      </value>
    </nameValuePair>

    <nameValuePair>
      <name>DeCafListFile</name>
      <value>
        <string>/caffe/extracted_feats/pnp_exp_bvlc_fc7/CNN_pnp_breakfast_1.list</string>
      </value>
    </nameValuePair>

    <nameValuePair>
      <name>DeCafKDTreeIndices</name>
      <value>
        <string>/caffe/extracted_feats/pnp_exp_bvlc_fc7/CNN_pnp_breakfast_1.idx</string>
      </value>
    </nameValuePair>

    <nameValuePair>
      <name>camera_config_files</name>
      <value>
       <array>
<!--         <string>config_kinect_robot.ini</string>-->
         <string>config_mongodb_webdemo.ini</string>
       </array>
      </value>
    </nameValuePair>

    <nameValuePair>
      <name>clearStorageOnStart</name>
      <value>
        <boolean>true</boolean>
      </value>
    </nameValuePair>

   <nameValuePair>
        <name>storagedb</name>
        <value>
          <string>ScenesEaseKickoffPnP</string>
        </value>
   </nameValuePair>

      <nameValuePair>
        <name>hsvFilter</name>
        <value>
          <boolean>true</boolean>
        </value>
      </nameValuePair>
      
      <nameValuePair>
        <name>segmMode</name>
        <value>
          <string>INVBINARY</string>
        </value>
      </nameValuePair>

      <nameValuePair>
        <name>cluster_min_size</name>
        <value>
          <integer>500</integer>
        </value>
      </nameValuePair>

      <nameValuePair>
        <name>minSize</name>
        <value>
          <integer>1200</integer>
        </value>
      </nameValuePair>

      <nameValuePair>
        <name>border</name>
        <value>
          <float>0.01</float>
        </value>
      </nameValuePair>

      <nameValuePair>
        <name>enable_change_detection</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>

    </configurationParameterSettings>
    <flowConstraints>
    <fixedFlow>
      <node>CollectionReader</node>
      <node>ImagePreprocessor</node>
      <node>RegionFilter</node>
      <node>NormalEstimator</node>
      <node>PlaneAnnotator</node>
      <node>PointCloudClusterExtractor</node>
      <node>ImageSegmentationAnnotator</node>
      <node>ClusterMerger</node>
      <node>ClusterFilter</node>
      <node>Cluster3DGeometryAnnotator</node>
      <node>PCLDescriptorExtractor</node>
      <node>ClusterTFLocationAnnotator</node>
      <node>ClusterColorHistogramCalculator</node>
      <node>PrimitiveShapeAnnotator</node>
      <node>ObjectIdentityResolution</node>
<!--      <node>BeliefToKnowRob</node>
      <node>GetRenderedViews</node>-->
<!--      <node>StorageWriter</node>-->
    </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection>
      <fsIndexes>
        <fsIndexDescription>
          <label>general</label>
          <typeName>uima.cas.TOP</typeName>
          <kind>bag</kind>
        </fsIndexDescription>
      </fsIndexes>
    </fsIndexCollection>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported>
          <language>x-unspecified</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
</taeDescription>
